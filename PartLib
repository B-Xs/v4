local a=loadstring(game:HttpGet("https://raw.githubusercontent.com/mstudio45/MSESP/refs/heads/main/source.luau"))()getgenv().PartsESP={Computers={Enabled=false,Highlight=false,HighlightColor=Color3.fromRGB(0,255,155),Name=false},FreezePods={Enabled=false,Highlight=false,HighlightColor=Color3.fromRGB(120,200,255),Name=false}}local b=game:GetService("ReplicatedStorage")local c=game:GetService("Players")local c=c.LocalPlayer;local c={}local d;local e={Computers={},FreezePods={}}local function f()for a,a in ipairs(c)do a:Disconnect()end;c={}for a,a in pairs(e)do for a,a in pairs(a)do if a then a:Destroy()end end end;e={Computers={},FreezePods={}}end;local function g(b,c)if e[b][c]then return end;local d=getgenv().PartsESP[b]if not d or not d.Enabled or not d.Highlight then return end;local a=a:Add({Name=d.Name and b or"",Model=c,Color=d.HighlightColor,MaxDistance=1000,TextSize=17,ESPType="Highlight"})e[b][c]=a end;local function a(a)if not a then return end;d=a;for a,a in ipairs(a:GetChildren())do if a.Name=="ComputerTable"and getgenv().PartsESP.Computers.Enabled then g("Computers",a)elseif a.Name=="FreezePod"and getgenv().PartsESP.FreezePods.Enabled then g("FreezePods",a)end end;table.insert(c,a.ChildAdded:Connect(function(a)if a.Name=="ComputerTable"and getgenv().PartsESP.Computers.Enabled then g("Computers",a)elseif a.Name=="FreezePod"and getgenv().PartsESP.FreezePods.Enabled then g("FreezePods",a)end end))table.insert(c,a.ChildRemoved:Connect(function(a)for b,b in pairs(e)do if b[a]then b[a]:Destroy()b[a]=nil end end end))end;table.insert(c,b.CurrentMap.Changed:Connect(function(b)f()if getgenv().PartsESP.Computers.Enabled or getgenv().PartsESP.FreezePods.Enabled then a(b)end end))task.spawn(function()while true do local a=b.CurrentMap.Value;if a then d=a;if getgenv().PartsESP.Computers.Enabled then for a,a in ipairs(a:GetChildren())do if a.Name=="ComputerTable"and not e.Computers[a]then g("Computers",a)end end else for a,a in pairs(e.Computers)do if a then a:Destroy()end end;e.Computers={}end;if getgenv().PartsESP.FreezePods.Enabled then for a,a in ipairs(a:GetChildren())do if a.Name=="FreezePod"and not e.FreezePods[a]then g("FreezePods",a)end end else for a,a in pairs(e.FreezePods)do if a then a:Destroy()end end;e.FreezePods={}end end;for a,b in pairs(e.Computers)do if a and a.Parent and b then local c=getgenv().PartsESP.Computers.HighlightColor;local a=a:FindFirstChild("Screen")if a and a:IsA("BasePart")then if a.BrickColor==BrickColor.new("Dark green")then c=Color3.fromRGB(0,200,0)b.CurrentSettings.Name=getgenv().PartsESP.Computers.Name and"Hacked Computer"or""else b.CurrentSettings.Name=getgenv().PartsESP.Computers.Name and"Computer"or""end else b.CurrentSettings.Name=getgenv().PartsESP.Computers.Name and"Computer"or""end;b.CurrentSettings.FillColor=c;b.CurrentSettings.OutlineColor=c;b.CurrentSettings.Color=c end end;for a,b in pairs(e.FreezePods)do if a and a.Parent and b then local a=getgenv().PartsESP.FreezePods.HighlightColor;b.CurrentSettings.FillColor=a;b.CurrentSettings.OutlineColor=a;b.CurrentSettings.Name=getgenv().PartsESP.FreezePods.Name and"Freeze Pod"or""b.CurrentSettings.Color=a end end;task.wait(0.05)end end)
